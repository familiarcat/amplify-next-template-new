{
  "name": "aws-amplify-gen2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:production": "node scripts/build-production.js",
    "build:custom": "node scripts/build-custom.js",
    "start": "next start",
    "lint": "next lint",
    "dev:local": "cross-env AWS_PROFILE=AmplifyUser npm-run-all --parallel sandbox dev",
    "sandbox": "cross-env AWS_PROFILE=AmplifyUser ampx sandbox",
    "setup": "node scripts/setup-local.js",
    "setup:auth": "node scripts/setup-auth.js",
    "init": "node scripts/init-amplify.js",
    "deploy": "cross-env AWS_PROFILE=AmplifyUser npm-run-all build:production deploy:backend deploy:frontend",
    "deploy:backend": "cross-env AWS_PROFILE=AmplifyUser ampx backend deploy",
    "deploy:frontend": "cross-env AWS_PROFILE=AmplifyUser ampx app deploy",
    "deploy:all": "node scripts/deploy-all.js",
    "sync-data": "node scripts/sync-data.js"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.11.0",
    "@aws-amplify/ui-react-ai": "^1.4.0",
    "@aws-amplify/ui-react-storage": "^3.10.0",
    "aws-amplify": "^6.14.2",
    "@aws-amplify/adapter-nextjs": "^1.0.19",
    "next": "14.2.10",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.15.0",
    "@aws-amplify/backend-ai": "^1.3.1",
    "@aws-amplify/backend-cli": "^1.5.0",
    "@aws-sdk/client-cognito-identity": "^3.540.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.540.0",
    "@aws-sdk/client-dynamodb": "^3.540.0",
    "@aws-sdk/lib-dynamodb": "^3.540.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "aws-cdk": "^2",
    "aws-cdk-lib": "^2",
    "constructs": "^10.3.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.23.1",
    "npm-run-all": "^4.1.5",
    "simple-git": "^3.23.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  }
}
